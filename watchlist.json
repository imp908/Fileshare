											from cte
											group by CityName
												having Max(PeopleCount)  = (
													select 
														Max(PeopleCount) as PeopleCount
													from cte	
												)
										) t on t.CityName = c1.CityName ;



										select * from MaxCountriesPopulation;
										select * from LargestCityInCountryByPopulation;

								]
								
